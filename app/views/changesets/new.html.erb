<h1>New changeset</h1>

<% form_for(@changeset) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :revision %><br />
    <%= f.text_field :revision %>
  </p>
  <p>
    <%= f.label :ticket %><br />
    <%= f.text_field :ticket %>
  </p>
  <p>
    <%= f.label :merged %><br />
    <%= f.check_box :merged %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_field :notes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', changesets_path %>