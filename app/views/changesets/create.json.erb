<% if @changeset.valid? %>
  {
    "success": true,
    "changeset": <%= render(:partial => 'changeset.html.erb', :locals => { :changeset => @changeset }).to_json.html_safe %>
  }
<% else %>
  {
    "success": false,
    "errors": <%= @changeset.errors.full_messages.to_json.html_safe %>
  }
<% end %>
